<section class="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-12">
        <div class="card">
          <div class="card-header">
            <h3 class="card-title">
              Danh sach tat ca Hoc sinh cua <%= class1.name %>
            </h3>
          </div>
          <div class="card-body">
            <div class="row mb-3">
              <div class="col-6">
                <a href="" class="btn btn-info mr-2">Export excel</a>
              </div>
            </div>

            <!-- /.card-header -->
            <div class="card-body">
              <table id="example2" class="table table-bordered table-hover">
                <thead>
                  <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Chi tiet hoc vien</th>
                  </tr>
                </thead>
                <tbody>
                  <% students.forEach(row => { %>
                  <tr>
                    <td><%= row.id %></td>
                    <td><%= row.name %></td>
                    <td><%= row.email %></td>

                    <td>
                      <a href="/admin/class/manage-student/student-detail/<%= row.id %>" class="btn btn-primary mr-2"
                        >Chi tiet hoc vien</a
                      >
                    </td>
                  </tr>
                  <% }); %>
                </tbody>
              </table>
            </div>
            <!-- /.card-body -->
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
