<h5 class="mb-2 mt-4">Danh sách lớp học đã tham gia:</h5>
<% classes.forEach(classStudent => { %>
<div class="col-lg-3 col-6">
  <div class="small-box bg-info">
    <div class="inner">
      <h3><%= classStudent.name %></h3>
    </div>
    <div class="icon">
      <i class="fas fa-chalkboard"></i>
    </div>
    <a href="/student/class/detail/<%= classStudent.id%>" class="small-box-footer">
      Thông tin chi tiết <i class="fas fa-arrow-circle-right"></i>
    </a>
  </div>
</div>
<% }); %>